<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

  <style name="EditTextGraphTheme" parent="android:Widget.EditText">
	  <item name="android:background">@drawable/graphtheme_edit_text_holo_dark</item>
	  <item name="android:textColor">#ffffff</item>
  </style>

  <style name="AutoCompleteTextViewGraphTheme" parent="android:Widget.AutoCompleteTextView">
      <item name="android:dropDownSelector">@drawable/graphtheme_list_selector_holo_dark</item>
	  <item name="android:background">@drawable/graphtheme_edit_text_holo_dark</item>
	  <item name="android:textColor">#ffffff</item>
  </style>

  <style name="CheckBoxGraphTheme" parent="android:Widget.CompoundButton.CheckBox">
      <item name="android:button">@drawable/graphtheme_btn_check_holo_dark</item>
  </style>

  <style name="RadioButtonGraphTheme" parent="android:Widget.CompoundButton.RadioButton">
      <item name="android:button">@drawable/graphtheme_btn_radio_holo_dark</item>
  </style>

  <style name="ButtonGraphTheme" parent="android:Widget.Button">
	  <item name="android:background">@drawable/graphtheme_btn_default_holo_dark</item>
	  <item name="android:minHeight">48dip</item>
	  <item name="android:minWidth">64dip</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="ImageButtonGraphTheme" parent="android:Widget.ImageButton">
	  <item name="android:background">@drawable/graphtheme_btn_default_holo_dark</item>
  </style>

  <style name="SpinnerGraphTheme" parent="android:Widget.Spinner">
      <item name="android:background">@drawable/graphtheme_spinner_background_holo_dark</item>
      <item name="android:dropDownSelector">@drawable/graphtheme_list_selector_holo_dark</item>
  </style>

  <style name="SpinnerDropDownItemGraphTheme" parent="android:Widget.DropDownItem.Spinner">
      <item name="android:checkMark">@drawable/graphtheme_btn_radio_holo_dark</item>
  </style>
  <style name="TabGraphTheme">
      <item name="android:gravity">center_horizontal</item>
      <item name="android:paddingLeft">16dip</item>
      <item name="android:paddingRight">16dip</item>
      <item name="android:background">@drawable/graphtheme_tab_indicator_holo</item>
      <item name="android:layout_width">0dip</item>
      <item name="android:layout_weight">1</item>
      <item name="android:minWidth">80dip</item>
  </style>

  <style name="TabTextGraphTheme">
	  <item name="android:textColor">#ffffff</item>
      <item name="android:textSize">12sp</item>
      <item name="android:textStyle">bold</item>
      <!-- v14 <item name="android:textAllCaps">true</item> -->
      <item name="android:ellipsize">marquee</item>
      <item name="android:maxLines">2</item>
      <item name="android:maxWidth">180dip</item>
  </style>

  <style name="ProgressBarGraphTheme" parent="android:Widget.ProgressBar.Horizontal">
      <item name="android:progressDrawable">@drawable/graphtheme_progress_horizontal_holo_dark</item>
      <item name="android:indeterminateDrawable">@drawable/graphtheme_progress_indeterminate_horizontal_holo_dark</item>
      <item name="android:minHeight">16dip</item>
      <item name="android:maxHeight">16dip</item>
  </style>

  <style name="ToggleGraphTheme" parent="android:Widget.Button.Toggle">
      <item name="android:background">@drawable/graphtheme_btn_toggle_holo_dark</item>
      <item name="android:minHeight">48dip</item>
	  <item name="android:textColor">#ffffff</item>
  </style>

  <style name="ListViewGraphTheme" parent="android:Widget.ListView">
      <item name="android:listSelector">@drawable/graphtheme_list_selector_holo_dark</item>
  </style>

  <style name="ListViewGraphTheme.White" parent="android:Widget.ListView.White">
      <item name="android:listSelector">@drawable/graphtheme_list_selector_holo_dark</item>
  </style>

  <style name="SpinnerItemGraphTheme" parent="android:TextAppearance.Widget.TextView.SpinnerItem">
      <item name="android:textColor">#ffffff</item>
  </style>

</resources>